{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "itemId": {
      "type": "biginteger"
    },
    "transMog": {
      "type": "boolean"
    },
    "usableByClass": {
      "type": "customField",
      "options": [
        "Barbarian",
        "Druid",
        "Necromancer",
        "Rogue",
        "Sorcerer"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "iconId": {
      "type": "biginteger"
    },
    "itemImages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::item-image.item-image",
      "mappedBy": "item"
    },
    "collectionItems": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection-item.collection-item",
      "mappedBy": "items"
    },
    "itemType": {
      "type": "string"
    },
    "magicType": {
      "type": "string"
    },
    "transmogName": {
      "type": "string"
    },
    "series": {
      "type": "string"
    }
  }
}
